{
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "installCommand": "npm install",
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        },
        {
          "key": "Pragma",
          "value": "no-cache"
        },
        {
          "key": "Expires",
          "value": "0"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/generate",
      "destination": "/generate"
    },
    {
      "source": "/dashboard",
      "destination": "/dashboard"
    },
    {
      "source": "/api/wellness/generate",
      "destination": "/api/wellness/generate"
    }
  ]
} 